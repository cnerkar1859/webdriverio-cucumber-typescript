{
  "name": "wdio-app-pom_framework",
  "version": "1.0.0",
  "description": "Test framework for automating mobile apps with appium using webdriverio & typescript!",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf typeScript/",
    "clean-build": "npm run clean && npm run build",
    "appium": "appium",
    "app-test": "wdio ./config/wdio.app.config.js",
    "browser-test": "wdio ./config/wdio.browser.config.js",
    "pretest": "npm run clean-build",
    "test": "npm run app-test && npm run browser-test",
    "report": "allure generate ./allure-results"
  },
  "author": "Chats Nerkar",
  "license": "ISC",
  "devDependencies": {
    "@tsconfig/node12": "^1.0.7",
    "@types/chai": "^4.2.14",
    "@types/cucumber": "^6.0.1",
    "@types/jquery": "^3.5.4",
    "@typescript-eslint/eslint-plugin": "^4.7.0",
    "@typescript-eslint/parser": "^4.7.0",
    "@wdio/allure-reporter": "^6.7.2",
    "@wdio/appium-service": "^6.7.3",
    "@wdio/cucumber-framework": "^6.7.3",
    "@wdio/local-runner": "^6.7.3",
    "@wdio/selenium-standalone-service": "^6.7.3",
    "@wdio/sync": "^6.7.3",
    "appium": "^1.18.3",
    "chai": "^4.2.0",
    "chai-webdriverio": "^1.0.0",
    "chromedriver": "^86.0.0",
    "cucumber": "^7.0.0-rc.0",
    "cucumber-pretty": "^6.0.0",
    "cucumber-tsflow": "^4.0.0-rc.1",
    "local-runner": "^0.1.1-alpha",
    "ts-node": "^9.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.0.5",
    "wdio-chromedriver-service": "^6.0.4",
    "webdriverio": "^6.7.3"
  },
  "dependencies": {
    "@wdio/cli": "^6.7.4",
    "awesome-typescript-loader": "^5.2.1",
    "expect-webdriverio": "^1.4.1",
    "source-map-loader": "^1.1.2"
  }
}
